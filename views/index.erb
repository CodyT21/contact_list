<ul style="list-style:none">
  <% @contacts.each_with_index do |contact_info, contact_id| %>
    <li>
      <h4><%= contact_info[:first_name].capitalize + ' ' + contact_info[:last_name].capitalize %></h4>
      <ul>
        <li>Phone Number: <%=  contact_info[:phone_number] %></li>
        <li>Email Address: <%= contact_info[:email_address] %></li>
        <p>
          <form action="/contacts/<%= contact_id %>/destroy" method="post">
            <button type="submit">Delete Contact</button>
          </form>
          <a href="/contacts/<%= contact_id %>/edit">
            <button>Edit Contact</button>
          </a>
        </p>
      </ul>
    </li>
  <% end %>
</ul>

<a href="/contacts/new">New Contact</a>
